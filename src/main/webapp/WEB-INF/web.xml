<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Web Application from Archetype for Servlet 4.0</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<request-character-encoding>UTF-8</request-character-encoding>
	<response-character-encoding>UTF-8</response-character-encoding>

	<servlet>
		<servlet-name>registerSLServlet</servlet-name>
		<servlet-class>web.servlet.RegisterSLServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registerSLServlet</servlet-name>
		<url-pattern>/registerSL</url-pattern>
		<url-pattern>/REGISTER_SL</url-pattern>
	</servlet-mapping>
	

</web-app>
